<Window x:Class="TravelNew.Currency"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Currency" Height="170" Width="500">
    <Grid Margin="0,0,0,-4">
        <Image x:Name="currency" HorizontalAlignment="Left" Height="170" VerticalAlignment="Top" Width="500" Source="pict/currency.jpg" Margin="0,-15,-8,-11"/>

        <TextBlock x:Name="name_curr" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="40,20,0,0" Text="НАЗВАНИЕ ВАЛЮТЫ" FontFamily="Cambria" FontSize="18" Width="436" Height="37" Foreground="White"/>
        <TextBlock x:Name="name_curr2" HorizontalAlignment="Left" TextWrapping="Wrap" Text="НАЗВАНИЕ ВАЛЮТЫ" VerticalAlignment="Top" Margin="40,62,0,0" FontFamily="Cambria" FontSize="18" Width="436" Height="45" Foreground="White"/>

    </Grid>
</Window>
